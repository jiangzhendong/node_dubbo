<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="autoMappingBehavior" value="PARTIAL" />
		<setting name="defaultExecutorType" value="SIMPLE" /><!-- SIMPLE REUSE BATCH -->
		<!-- <setting name="defaultExecutorType" value="BATCH" /> -->
		<setting name="defaultStatementTimeout" value="25000" />
		<setting name="safeRowBoundsEnabled" value="false" />
		<setting name="mapUnderscoreToCamelCase" value="false" />
		<setting name="localCacheScope" value="SESSION" />
		<!-- <setting name="jdbcTypeForNull" value="OTHER" /> -->
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString" />
	</settings>
	<typeAliases>
		<!-- =========================================================== -->
	    <!-- security模块                                                                                                                                                                             -->
	    <!-- =========================================================== -->
		<typeAlias alias="operationLog" type="com.haier.openplatform.log.domain.OperationLog" />
		<typeAlias alias="operationLogSearchModel" type="com.haier.openplatform.showcase.security.model.OperationLogSearchModel"/>
		<typeAlias alias="uploadFile" type="com.haier.openplatform.showcase.security.domain.UploadFile"/>
		<typeAlias alias="fileSearchModel" type="com.haier.openplatform.showcase.security.model.FileSearchModel"/>
		<typeAlias alias="userDTO" type="com.haier.openplatform.showcase.security.dto.UserDTO"/>

		
		
		<!-- =========================================================== -->
	    <!-- apportion模块                                                                                                                                                                             -->
	    <!-- =========================================================== -->
		<typeAlias alias="companyProportion" type="com.haier.openplatform.showcase.apportion.domain.CompanyProportion"/>
		<typeAlias alias="apportionMoney" type="com.haier.openplatform.showcase.apportion.domain.ApportionMoney"/>

		<typeAlias alias="weight" type="com.haier.openplatform.showcase.apportion.domain.Weight"/>
		<typeAlias alias="companyData" type="com.haier.openplatform.showcase.apportion.domain.CompanyData"/>
		<typeAlias alias="coefficient" type="com.haier.openplatform.showcase.apportion.domain.Coefficient"/>
		<typeAlias alias="platform" type="com.haier.openplatform.showcase.apportion.domain.Platform"/>
		<typeAlias alias="platformFile" type="com.haier.openplatform.showcase.apportion.domain.PlatformFile"/>
		<typeAlias alias="configFile" type="com.haier.openplatform.showcase.apportion.domain.ConfigFile"/>
		<typeAlias alias="companyPlatform" type="com.haier.openplatform.showcase.apportion.domain.CompanyPlatform"/>
		<typeAlias alias="companyDetails" type="com.haier.openplatform.showcase.apportion.domain.CompanyDetails"/>
		<typeAlias alias="ftzfDomain" type="com.haier.openplatform.showcase.apportion.domain.Ftzf"/>
		<typeAlias alias="companyPlatform" type="com.haier.openplatform.showcase.apportion.domain.CompanyPlatform"/>


	</typeAliases>
	
	<typeHandlers>
	  <typeHandler handler="com.haier.openplatform.dao.mybatis.SerializableTypeHandler"/>
	</typeHandlers>
</configuration>
